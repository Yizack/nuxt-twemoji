{
  "name": "nuxt-twemoji",
  "version": "3.6.1",
  "description": "Twemoji module for Nuxt. Render emojis as SVG elements or PNG images",
  "keywords": [
    "nuxt",
    "twemoji",
    "emoji",
    "emojis",
    "nuxt3",
    "twitter-emoji"
  ],
  "repository": "Yizack/nuxt-twemoji",
  "homepage": "https://github.com/Yizack/nuxt-twemoji",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com/"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./emojis": {
      "types": "./dist/runtime/assets/emojis.d.ts",
      "import": "./dist/runtime/assets/emojis.js"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "emojis": [
        "./dist/runtime/assets/emojis.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit",
    "emojis": "node ./scripts/generateEmojis.js"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "@twemoji/api": "^15.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.10",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "nuxt": "^3.12.3",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.26"
  },
  "packageManager": "pnpm@9.5.0",
  "changelog": {
    "repo": {
      "repo": "Yizack/nuxt-twemoji",
      "provider": "github",
      "domain": "github.com"
    }
  }
}
