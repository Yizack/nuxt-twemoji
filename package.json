{
  "name": "nuxt-twemoji",
  "version": "3.6.3",
  "description": "Twemoji module for Nuxt. Render emojis as SVG elements or PNG images",
  "keywords": [
    "nuxt",
    "twemoji",
    "emoji",
    "emojis",
    "nuxt3",
    "twitter-emoji"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/nuxt-twemoji.git"
  },
  "homepage": "https://github.com/Yizack/nuxt-twemoji",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com/"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./emojis": {
      "types": "./dist/runtime/assets/emojis.d.ts",
      "import": "./dist/runtime/assets/emojis.js"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "emojis": [
        "./dist/runtime/assets/emojis.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit",
    "emojis": "jiti ./scripts/generateEmojis && npm run lint:fix"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0",
    "@twemoji/api": "^15.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.2.1",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "^22.13.10",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "nuxt": "^3.16.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.6.2",
  "changelog": {
    "repo": {
      "repo": "Yizack/nuxt-twemoji",
      "provider": "github",
      "domain": "github.com"
    }
  }
}
