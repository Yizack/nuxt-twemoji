{
  "name": "nuxt-twemoji",
  "version": "3.2.4",
  "description": "Twemoji module for Nuxt. Render emojis as SVG elements or PNG images",
  "keywords": [
    "nuxt",
    "twemoji",
    "emoji",
    "emojis",
    "nuxt3",
    "twitter-emoji"
  ],
  "repository": "Yizack/nuxt-twemoji",
  "homepage": "https://github.com/Yizack/nuxt-twemoji",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./emojis": {
      "types": "./dist/runtime/assets/emojis.d.ts",
      "import": "./dist/runtime/assets/emojis.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "pack": "pnpm lint && pnpm test && pnpm prepack",
    "release": "pnpm pack && changelogen --release && pnpm release:publish",
    "release:minor": "pnpm pack && changelogen --release --minor && pnpm release:publish",
    "release:major": "pnpm pack && changelogen --release --major && pnpm release:publish",
    "release:publish": "npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "emojis": "node ./scripts/generateEmojis.js"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/schema": "^3.5.1",
    "@nuxt/test-utils": "^3.5.1",
    "@types/node": "^18.16.13",
    "changelogen": "^0.5.3",
    "eslint": "^8.41.0",
    "nuxt": "^3.5.1",
    "vitest": "^0.31.1"
  },
  "changelog": {
    "repo": {
      "repo": "Yizack/nuxt-twemoji",
      "provider": "github",
      "domain": "github.com"
    }
  }
}