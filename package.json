{
  "name": "nuxt-twemoji",
  "version": "3.0.0",
  "description": "Twemoji module for Nuxt",
  "keywords": [
    "nuxt",
    "twemoji",
    "emoji",
    "emojis",
    "nuxt3",
    "twitter-emoji"
  ],
  "repository": "Yizack/nuxt-twemoji",
  "homepage": "https://github.com/Yizack/nuxt-twemoji",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "pack": "npm run lint && npm run test && npm run prepack",
    "release": "npm run pack && changelogen --release && npm run release:publish",
    "release:minor": "npm run pack && changelogen --release --minor && npm run release:publish",
    "release:major": "npm run pack && changelogen --release --major && npm run release:publish",
    "release:publish": "npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/schema": "^3.5.0",
    "@nuxt/test-utils": "^3.5.0",
    "@types/node": "^18",
    "changelogen": "^0.5.3",
    "eslint": "^8.40.0",
    "nuxt": "^3.5.0",
    "vitest": "^0.31.0"
  }
}